<header>
    <nav class="navbar navbar-expand-md navbar-dark bg-success">
      <a class="navbar-brand" href="/urls">TinyApp</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div style="display:flex; justify-content:space-between" class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <a class="nav-item nav-link" href="/urls">My URLs</a>
          <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        </div>
        <% if(user) { %>
          <form class="form-inline" action="/logout" method="POST">
            <h5>Logged in as: <%= user.email %>!&nbsp</h5>
            <div class="form-group mb-2">
              <button type="submit" class="btn btn-dark">Logout</button>
            </div>
          </form>
        <% } else { %>
          <div class="navbar-nav">
          <a style="color:blueviolet; font-weight: 800; margin-right:20px;" href="/login">Login</a>
          <a style="color:gold; font-weight: 800;"href="/register">Register Here</a>
         </div>
        <% } %>
      </div>
    </nav>
  </header>

  